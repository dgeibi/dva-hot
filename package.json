{
  "name": "dva-hot",
  "version": "0.1.0",
  "description": "HMR plugin for dva without babel",
  "main": "lib/index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib",
    "precommit": "lint-staged",
    "format": "prettier --write \"**/*.js\"",
    "test": "jest",
    "lint": "eslint ."
  },
  "author": "dgeibi",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.37",
    "@babel/core": "^7.0.0-beta.37",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.37",
    "@babel/preset-env": "^7.0.0-beta.37",
    "@easyops/git-exec-and-restage": "^1.0.4",
    "@types/history": "^4.6.2",
    "@types/react": "^16.0.34",
    "@types/redux": "^3.6.0",
    "@types/webpack-env": "^1.13.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-dgeibi": "^4.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "jest-plugin-console-matchers": "^2.6.0",
    "jest-plugins": "^2.6.0",
    "lint-staged": "^6.0.0",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "redbox-react": "^1.5.0"
  },
  "directories": {
    "lib": "lib"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 90
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.37"
  },
  "peerDependencies": {
    "react": "15.x || 16.x",
    "react-dom": "15.x || 16.x",
    "redbox-react": "1.x"
  },
  "files": [
    "lib/",
    "dva.d.ts",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgeibi/dva-hot.git"
  },
  "keywords": [
    "dva",
    "hmr"
  ],
  "bugs": {
    "url": "https://github.com/dgeibi/dva-hot/issues"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git-exec-and-restage prettier --write --"
    ]
  },
  "homepage": "https://github.com/dgeibi/dva-hot#readme",
  "jest": {
    "setupTestFrameworkScriptFile": "./jest-plugins.js"
  }
}
