{
  "name": "dva-hot",
  "version": "0.2.2",
  "description": "HMR plugin for dva without babel",
  "typings": "./index.d.ts",
  "scripts": {
    "build": "rimraf lib && babel src --out-dir lib",
    "precommit": "lint-staged",
    "format": "prettier --write \"**/*.js\"",
    "test": "jest",
    "lint": "eslint .",
    "example": "webpack-dev-server --config ./example/webpack.config.js"
  },
  "author": "dgeibi",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "@easyops/git-exec-and-restage": "^1.0.4",
    "@types/history": "^4.6.2",
    "@types/react": "^16.3.9",
    "@types/webpack-env": "^1.13.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.0-beta.3",
    "dva": "^2.3.0",
    "eslint": "^4.19.1",
    "eslint-config-dgeibi": "^5.0.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "fs-extra": "^5.0.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-plugin-console-matchers": "^2.9.0",
    "jest-plugins": "^2.9.0",
    "lint-staged": "^7.1.3",
    "prettier": "^1.12.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.3"
  },
  "directories": {
    "lib": "lib"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 90
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.49"
  },
  "peerDependencies": {
    "react": "15.x || 16.x",
    "react-dom": "15.x || 16.x"
  },
  "files": [
    "lib/",
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgeibi/dva-hot.git"
  },
  "keywords": [
    "dva",
    "hmr"
  ],
  "bugs": {
    "url": "https://github.com/dgeibi/dva-hot/issues"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git-exec-and-restage prettier --write --"
    ]
  },
  "homepage": "https://github.com/dgeibi/dva-hot#readme",
  "jest": {
    "setupTestFrameworkScriptFile": "./jest-plugins.js"
  }
}
